pkgname=abench
pkgver=git
pkgrel=1
arch=('i386' 'x86_64' 'riscv64' 'aarch64')
url="git://sos-git.amd.com/linux-tools.git"
license=('GPL')
source=("git://sos-git.amd.com/linux-tools.git")
md5sums=('SKIP')

build() {
       cd "${srcdir}/linux-tools/mm/${pkgname}"
       make
}

package() {
       mkdir -p "${pkgdir}/lkp/benchmarks/${pkgname}"
       cp -r "${srcdir}/linux-tools/mm/${pkgname}" "${pkgdir}/lkp/benchmarks/${pkgname}"
}
